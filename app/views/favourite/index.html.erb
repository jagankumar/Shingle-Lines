<div><%= link_to "Home", images_index_path %></div>
<h1>Your Favourites</h1>

<%@favourites.select("DISTINCT img_url,image_name").each do|fav|  %>
<div>
  <img src="<%= fav.img_url %>" style="width:30%">
  </div><br>
  <div><strong>Total Likes<%@like = Like.where(:img_url => fav.img_url).first %> <%= @like.like_count%> -- <%= fav.image_name %></strong>
    </div>

<% end %>